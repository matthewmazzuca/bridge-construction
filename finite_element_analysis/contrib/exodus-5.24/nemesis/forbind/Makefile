.SUFFIXES: .F .f .f90 .c .C .cpp
.c.o:
	$(CC) -c $(CFLAGS) $*.c
.C.o:
	$(CXX) -c $(CXXFLAGS) $*.C
.cpp.o:
	$(CXX) -c $(CXXFLAGS) $*.cpp
.f.o:
	$(FC) -c $(STATICFLAGS) $(FFLAGS) $*.f
.f90.o:
	$(F90C) -c $(STATICFLAGS) $(F90FLAGS) $*.f90
.f90.mod:
	$(F90C) -c $(STATICFLAGS) $(F90FLAGS) $*.f90
.mod.o:
.F.o:
	$(FC) -c $(STD_CPP_DEFINES) $(F77CPPFLAGS) $(DEFINES) $(STATICFLAGS) $(FFLAGS) $*.F

