	$(CC) -o ne_jack32.o -c $(CFLAGS) -DDEFAULT_REAL_INT ne_jack.c

all:: libnemIf32.a
libnemIf32.a: ne_jack32.o
	$(AR) $@ $?
	$(_NULLCMD_)
	$(_NULLCMD_)
	$(CP) $@ ../

